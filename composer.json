{
	"name":"fritzmg/contao-news-image-override",
	"description":"Allows the image settings of each news entry in Contao to override the image settings of the news module",
	"keywords":["contao", "news", "image", "override", "module"],
	"type":"contao-module",
	"homepage":"http://www.inspiredminds.at",
	"license":"GPL-2.0",
	"authors":[
		{
			"name":"Fritz Michael Gschwantner",
			"homepage":"http://www.inspiredminds.at",
			"email":"fmg@inspiredminds.at",
			"role":"Developer"
		}
	],
	"support":{
		"email":"fmg@inspiredminds.at",
		"issues":"https://github.com/fritzmg/contao-news-image-override/issues",
		"source":"https://github.com/fritzmg/contao-news-image-override",
		"forum":"https://community.contao.org/de"
	},
	"require":{
		"php":">=5.2",
		"contao/core-bundle":"~3.0 || ~4.0",
		"contao-community-alliance/composer-plugin":"~2.4 || ~3.0"
	},
	"replace": {
		"contao-legacy/news_image_override": "self.version"
	},
	"extra":{
		"contao":{
			"sources":{
				"system/modules/news_image_override":"system/modules/news_image_override"
			}
		}
	}
}
